{
    "Settings": {
        "Version": "0.0.0.0",
        "MaximumMemoryUsage": 500,
        "MemoryMonitorInterval": 60,
        "LogType": 2, // 0: Console, 1: File, 2: ConsoleAndFile
        "NoteClassification": [
            "All",
            "Today",
            "Tomorrow",
            "LastSevenDays",
            "Assigned",
            "Collect",
            "Summary"
        ],
        "DataBase": 1, // 0: Sqlite, 1: SqlServer, 2: Mysql
        "ConnectionString": "Data Source=.\\DataBases\\Todo.db; Pooling=true;Min Pool Size=1",
        "ArchivedCollection": [
            "IsCompleted",
            "IsAbandon",
            "TrashBox"
        ]
    }
}